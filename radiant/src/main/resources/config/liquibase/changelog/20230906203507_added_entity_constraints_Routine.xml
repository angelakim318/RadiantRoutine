<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Routine.
    -->
    <changeSet id="20230906203507-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="added_by_id"
                                 baseTableName="routine"
                                 constraintName="fk_routine__added_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

        <addForeignKeyConstraint baseColumnNames="routine_id"
                                 baseTableName="rel_routine__product"
                                 constraintName="fk_rel_routine__product__routine_id"
                                 referencedColumnNames="id"
                                 referencedTableName="routine"/>

        <addForeignKeyConstraint baseColumnNames="product_id"
                                 baseTableName="rel_routine__product"
                                 constraintName="fk_rel_routine__product__product_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product"/>
    </changeSet>
</databaseChangeLog>
